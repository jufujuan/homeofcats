<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:padding="@dimen/fra_found_item_margin_small"
              android:orientation="vertical">

   <jfj.homeofcars.View.BorderImageView
       android:id="@+id/item_found_time_item_img"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"/>
   <TextView
       android:id="@+id/item_found_time_item_title"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"/>
   <TextView
       android:textColor="@color/fra_found_price"
       android:id="@+id/item_found_time_item_price"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"/>
</LinearLayout>